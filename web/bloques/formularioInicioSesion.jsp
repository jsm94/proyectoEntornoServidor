<div class="col-md-6 col-md-offset-3">
    <form role="form">
        <h2>Inicia sesión</h2>
        <div class="form-group">
            <label for="nick">Nombre de usuario <small>(nick)</small></label>
            <input type="text" class="form-control" id="nick" placeholder="Introduce tu nombre de usuario">
        </div>
        <div class="form-group">
            <label for="pass">Contraseña</label>
            <input type="password" class="form-control" id="pass" placeholder="Introduce tu contraseña">
        </div>
        <button type="submit" class="btn btn-primary">Entrar</button>
    </form>
</div>

